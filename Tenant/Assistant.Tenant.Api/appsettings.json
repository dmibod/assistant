{
  "NatsSettings": {
    "Url": "nats://localhost",
    "User": "user",
    "Password": "password",
    "StockCreateTopic": "stock.create",
  },
  "DatabaseSettings": {
    "ConnectionString": "mongodb://localhost",
    "DatabaseName": "db",
    "TenantCollectionName": "tenant"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "System.Net.Http.HttpClient": "Warning"
      },
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "MMM-dd-yy HH:mm:ss.fff ",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*"
}
